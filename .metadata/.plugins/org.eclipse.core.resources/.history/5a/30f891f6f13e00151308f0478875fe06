import javax.swing.JOptionPane;

public class August_9th {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		// Copyright The League of Amazing Programmers 2014
		

		/*
		 * I have a pocket full of change. I hate doing math. Make me a program that
		 * will calculate how much money I have without me having to use my brain. Then
		 * make me a sandwich.
		 */
		 

		

				
String nickles= JOptionPane.showInputDialog("how many nickels do ya own sonnie?");
 int nickle= Integer.parseInt(nickles);
			
 String dimes = JOptionPane.showInputDialog("how many dimes do ya own chiiiild?");
 int dime= Integer.parseInt(dimes);
				
 String quarters = JOptionPane.showInputDialog("how many quarters do ya own kiddie?");
 int quarter= Integer.parseInt(quarters);
				// Calculate how much money the user has and save it in a double variable 
 double totalmonies = (nickle*5 + quarter*25 + dime*10);
				// Tell the user how much money they have
 JOptionPane.showMessageDialog(null, "you have a total amount of "+totalmonies+"cents");
			


	}
}
